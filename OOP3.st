Object subclass: #MyObject	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'OOP3'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!MyObject class	instanceVariableNames: ''!!MyObject class methodsFor: 'compiling' stamp: 'SBK 12/14/2019 20:25'!compile: aSourceCode where: anOrderedCollection	| lineTokens numberOfArgs pred |	lineTokens := aSourceCode findTokens: (String with: Character cr).	numberOfArgs := (lineTokens at: 1) occurrencesOf: $:.		pred := (numberOfArgs = (anOrderedCollection size)).	pred ifFalse: [ self AssertionFailure signal: 'Can not compile method, number of arguments is not equal to the number of constraints!!' ].	^self compile: aSourceCode notifying: nil! !